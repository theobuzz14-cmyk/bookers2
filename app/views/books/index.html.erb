<h1>Books#index</h1>
<p>Find me in app/views/books/index.html.erb</p>




<h1>画像投稿フォーム</h1>
<%= form_with model: @book do |f| %>
  <h4>画像</h4>
  <%= f.file_field :image, accept: "image/*" %>
  <h4>本名</h4>
  <%= f.text_field :title %>
  <h4>感想</h4>
  <%= f.text_area :body %>
  <%= f.submit '投稿' %>
<% end %>